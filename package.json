{
	"name": "@hetchet/eslint-plugin-sort-keys",
	"version": "0.0.1",
	"private": false,
	"keywords": [
		"eslint",
		"eslintplugin",
		"hetchet",
		"javascript",
		"linting",
		"formatting"
	],
	"license": "MIT",
	"author": {
		"name": "Hetchet",
		"email": "senieck@gmail.com"
	},
	"main": "index.js",
	"files": [
		"src"
	],
	"prettier": "@hetchet/eslint-config-serenity/prettier",
	"eslintConfig": {
		"plugins": [
			"@hetchet/sort-keys"
		],
		"extends": "@hetchet/serenity/js",
		"rules": {
			"@hetchet/sort-keys/sort-keys": [
				"error",
				"asc",
				{
					"allowLineSeparatedGroups": true
				}
			]
		}
	},
	"devDependencies": {
		"@hetchet/eslint-config-serenity": "^0.0.15",
		"@hetchet/eslint-plugin-sort-keys": "file:.",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"babel-eslint": "^10.1.0",
		"escape-string-regexp": "4.0.0",
		"eslint": "^8.32.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-newline-destructuring": "^1.1.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-simple-import-sort": "^9.0.0",
		"espree": "^9.4.1",
		"esutils": "^2.0.3",
		"jest": "^29.3.1",
		"natural-compare": "^1.4.0",
		"postcss": "^8.4.21",
		"prettier": "^2.8.3",
		"prettier-eslint": "^15.0.1",
		"prettier-plugin-css-order": "^1.3.0",
		"prettier-plugin-packagejson": "^2.4.0"
	}
}
